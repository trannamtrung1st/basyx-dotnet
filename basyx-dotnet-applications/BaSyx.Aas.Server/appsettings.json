{
  "AppDataServiceSettings": {
    "ServiceConfig": {
      "AppName": "adminshell-registry"
    }
  },
  "ServerSettings": {
    "ServerConfig": {
      "Hosting": {
        "Environment": "Development",
        "ContentPath": "Content",
        "Urls": [
          "http://localhost:7777"
        ]
      },
      "DefaultRoute": "/ui",
      "PathBase": null
    },
    "Miscellaneous": {
      "CompanyLogo": "/images/MyCompanyLogo.png",
      "AutoRegister": false
    },
    "UserInterfaceConfig": {
      "Title": "AdminShell Registry",
      "BrandLogo": "/images/MyCompanyLogo.png",
      "Links": [
        {
          "Name": "AdminShellRegistry-UI",
          "Url": "/ui"
        }
      ]
    },
    "DiscoveryConfig": {
      "AutoDiscovery": false
    }
  },
  "RegistryClientSettings": {
    "ClientConfig": {
      "RequestConfig": {
        "RequestTimeout": 10000
      }
    },
    "RegistryConfig": {
      "RegistryUrl": "http://localhost:7777"
    },
    "ProxyConfig": {
      "UseProxy": false,
      "ProxyAddress": "http://localhost:7777"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}